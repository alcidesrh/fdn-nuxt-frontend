#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/esm/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/esm/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@parcel+watcher@2.5.1_@types+node@24.0.2_crossws@0.3.5_graphql@16.11.0_typescript@5.8.3/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
fi
