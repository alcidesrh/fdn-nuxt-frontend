#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alcides/fdn/pwa/node_modules/.pnpm/formkit@1.6.9/node_modules/formkit/bin/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/formkit@1.6.9/node_modules/formkit/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/formkit@1.6.9/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alcides/fdn/pwa/node_modules/.pnpm/formkit@1.6.9/node_modules/formkit/bin/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/formkit@1.6.9/node_modules/formkit/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/formkit@1.6.9/node_modules:/home/alcides/fdn/pwa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../formkit/bin/formkit.mjs" "$@"
else
  exec node  "$basedir/../formkit/bin/formkit.mjs" "$@"
fi
