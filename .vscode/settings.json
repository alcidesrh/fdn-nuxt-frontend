{
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 2000,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "editor.snippetSuggestions": "none",
  "css.hover.documentation": true,
  "css.hover.references": true,
  "html.hover.documentation": true,
  "html.hover.references": true,
  "less.hover.documentation": false,
  "less.hover.references": false,
  "scss.hover.documentation": false,
  "scss.hover.references": false,
  "editor.inlineSuggest.showToolbar": "onHover",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0a7d5f",
    "activityBar.background": "#0a7d5f",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#f8fafc",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0a7d5f",
    "statusBar.background": "#064e3b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0a7d5f",
    "statusBarItem.remoteBackground": "#064e3b",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#064e3b",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#064e3b99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "editor.suggest.showColors": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "html.suggest.html5": true,
  "markdown.suggest.paths.enabled": false,
  "editor.screenReaderAnnounceInlineSuggestion": true,
  "editor.inlineSuggest.enabled": true,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.suggest.showConstants": true,
  "editor.suggest.showCustomcolors": true,
  "editor.suggest.showEnumMembers": true,
  "editor.suggest.showEnums": true,
  "editor.suggest.showFields": true,
  "editor.suggest.showTypeParameters": true,
  // "nuxtr.intellisense.vueFiles": false,
  "vue.inlayHints.destructuredProps": true,
  "unocss.autocomplete.strict": false,
  "autoimport.autoComplete": false,
  "editor.suggest.showWords": false,
  "vue.autoInsert.dotValue": true,
  "typescript.tsserver.web.projectWideIntellisense.enabled": true,
  "editor.suggest.showSnippets": true,
  "emmet.showSuggestionsAsSnippets": true,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.suggest.filterGraceful": true,
  "editor.suggest.selectionMode": "always",
  "editor.suggest.showClasses": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.enabled": true,
  "javascript.suggest.names": true,
  "javascript.suggest.paths": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.enabled": true,
  "typescript.suggest.paths": true,
  "php.suggest.basic": false,
  // "unocss.disable": true,
  // "css.format.spaceAroundSelectorSeparator": true,
  // "tailwindcss.jsxPropImportChecking": false
}
